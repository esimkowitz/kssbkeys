{
	"name": "kssbkeys",
	"displayName": "KSSB Keys",
	"description": "Improving the VSCode experience for users with screen readers.",
	"version": "<insert here>",
	"publisher": "esimkowitz",
	"license": "MIT",
	"author": {
		"email": "esimkowitz@wustl.edu",
		"name": "Evan Simkowitz",
		"url": "https://github.com/esimkowitz"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/esimkowitz/kssbkeys.git"
	},
	"bugs": {
		"url": "https://github.com/esimkowitz/kssbkeys/issues"
	},
	"engines": {
		"vscode": "^1.48.0"
	},
	"extensionKind": [
		"ui"
	],
	"categories": [
		"Keymaps",
		"Other"
	],
	"keywords": [
		"screen reader",
		"accessibility"
	],
	"activationEvents": [
		"onStartupFinished",
		"onView:explorer",
		"onCommand:kssbkeys.explorerNewFolder"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "kssbkeys.explorerNewFolder",
				"title": "KSSB Keys: New Folder"
			}
		],
		"keybindings": [
			{
				"mac": "cmd+n",
				"win": "ctrl+n",
				"linux": "ctrl+n",
				"command": "explorer.newFile",
				"when": "explorerViewletVisible && filesExplorerFocus && !inputFocus"
			},
			{
				"mac": "cmd+shift+n",
				"win": "ctrl+shift+n",
				"linux": "ctrl+shift+n",
				"command": "kssbkeys.explorerNewFolder",
				"when": "explorerViewletVisible && filesExplorerFocus && !inputFocus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"test": "node ./out/test/runTest.js",
		"deploy": "vsce publish --yarn --pat $VSCE_PAT",
		"postinstall": "node ./node_modules/@types/vscode/bin/install"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"@types/vscode": "^1.48.0",
		"@typescript-eslint/eslint-plugin": "^3.8.0",
		"@typescript-eslint/parser": "^3.8.0",
		"eslint": "^7.6.0",
		"glob": "^7.1.6",
		"mocha": "^8.0.1",
		"typescript": "^3.8.3",
		"vsce": "^1.78.0",
		"vscode-test": "^1.4.0"
	}
}
